@model IList<Microsoft.AspNetCore.Identity.IdentityUser>

 
<h1 class=" text-center text-dark">Dashboard</h1>
<a asp-action="Create" class="btn btn-secondary">Create a Role</a>
<a asp-action="AssignRole" class="btn btn-secondary">Assign Role</a>

<table class=" table table-striped table-dark table table-sm table-bordered table-bordered">
    <tr><th>ID</th><th>Name</th><th>Phone Number</th></tr>

   

    @foreach (var role in Model)
    {
        <tr>
            <td>@role.Id</td>
            <td>@role.UserName</td>
             <td>@role.PhoneNumber</td>
            
            <td><a class="btn btn-sm btn-primary" asp-action="Update" asp-route-id="@role.Id">Update</a></td>
            <td>
                <form asp-action="Delete"  method="post">
                    <button type="submit" class="btn btn-sm btn-danger">
                        Delete
                    </button>
                </form>
            </td>
        </tr>
    }
</table>